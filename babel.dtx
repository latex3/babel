               \fi 
           \fi 
       \fi 
   \else 
       \advance #2 by 1
   \fi 
   \bbl@remainder{#2}{7}{\tmpa}%
   \ifnum \tmpa=0
       \advance #2 by 1 
   \else
       \ifnum \tmpa=3
           \advance #2 by 1 
       \else 
           \ifnum \tmpa=5
                \advance #2 by 1 
           \fi 
       \fi 
   \fi 
   \global\bbl@cntcommon=#2\relax}%
  #2=\bbl@cntcommon}
